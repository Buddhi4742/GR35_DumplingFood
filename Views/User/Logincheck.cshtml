@model Dumpling_Food.Models.User.LoginModel

@{
    ViewData["Title"] = "Logincheck";
    Layout = "~/Views/Shared/_LayoutList.cshtml";
}
<h1 class="header1">Welcome @Html.DisplayFor(model => model.custName)</h1>
<div>
    @*<h4>Welcome @Html.DisplayFor(model => model.custName)</h4>*@
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
           Profile Picture
        </dt>
        <dd class="col-sm-10">
            <img src="@Url.Content(Model.ImageUrl)" alt="Image" />
        </dd>
        <dt class="col-sm-2">
            Your Username
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.userName)
        </dd>
        <dt class="col-sm-2">
            Your ID
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.custId)
        </dd>
        <dt class="col-sm-2">
            Your Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.custName)
        </dd>
    </dl>
</div>

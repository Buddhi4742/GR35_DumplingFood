@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"/>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>

</head>
<body>
    <section id="slider">
        <div class="slider-carousel">
            <div id="carousel-fade" class="carousel slide carousel-fade" data-ride="carousel" data-interval="3500">
                <!-- Indicators -->
                <ol class="carousel-indicators">
                    <li type="button" data-bs-target="#carousel-fade" data-bs-slide-to="0" class="active"></li>
                    <li type="button" data-bs-target="#carousel-fade" data-bs-slide-to="1"></li>
                    <li type="button" data-bs-target="#carousel-fade" data-bs-slide-to="2"></li>
                </ol>
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img src="~/lib/Assets/images/slider-image-1.jpg" class="d-block w-100">
                        <div class="carousel-caption d-none d-md-block">
                            <h5 class="animate__animated animate__fadeInDown" style="animation-delay: 0.5s">Welcome to Dumpling Food Restaurant</h5>
                            <p class="animate__animated animate__fadeInUp" style="animation-delay:0.75s;">Authentique Chinese Cuisine.</p>
                            <button class="button button-slider-cta animate__animated animate__fadeInUp" style="animation-delay: 0.25s"><span>Learn More</span></button>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img src="~/lib/Assets/images/slider-image-2.jpg" class="d-block w-100">
                        <div class="carousel-caption d-none d-md-block">
                            <h5 class="animate__animated animate__fadeInDown" style="animation-delay: 0.5s">Welcome to Dumpling Food Restaurant</h5>
                            <p class="animate__animated animate__fadeInUp" style="animation-delay:0.75s;">Authentique Chinese Cuisine.</p>
                            <button class="button button-slider-cta animate__animated animate__fadeInUp" style="animation-delay: 0.25s"><span>Order Online</span></button>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img src="~/lib/Assets/images/slider-image-3.jpg" class="d-block w-100">
                        <div class="carousel-caption d-none d-md-block">
                            <h5 class="animate__animated animate__fadeInDown" style="animation-delay: 0.5s">Welcome to Dumpling Food Restaurant</h5>
                            <p class="animate__animated animate__fadeInUp" style="animation-delay: 0.75s;">Authentique Chinese Cuisine.</p>
                            <button class="button button-slider-cta animate__animated animate__fadeInUp " style="animation-delay: 0.25s"><span>Reserve Table</span></button>
                        </div>
                    </div>
                </div>
                <!-- Controls -->
                <a class="carousel-control-prev" data-bs-target="#carousel-fade" data-bs-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="visually-hidden"></span>
                </a>
                <a class="carousel-control-next" data-bs-target="#carousel-fade" data-bs-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="visually-hidden"></span>
                </a>
            </div>
        </div>
    </section>
    <section id="">
    </section>
    <div class="featured-food">

    </div>
    <div class="recomended-food">

    </div>
    <div class="resturent-features">

    </div>
    <div class="food-menu">

    </div>
    <div class="chefs">

    </div>
    <div class="location-map">

    </div>
    <div class="contact-newsletter">

    </div>
</body>
    </html>
